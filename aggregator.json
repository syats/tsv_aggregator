{
  "id": "/tsvaggregator",
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "vmireles/tsvaggregator",
      "forcePullImage": true
      }
  },
  "portMappings": [
      {
        "containerPort": 8765,
        "hostPort": 8765,
        "labels": {
          "VIP_0":"/tsvaggregator:8765"
        },
        "protocol":"tcp"
      }
  ],
  "cpus": 0.1,
  "mem": 128,
  "cmd": "python /server/server.py"
}